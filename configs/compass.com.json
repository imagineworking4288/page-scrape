{
  "domain": "compass.com",
  "name": "Compass Real Estate",
  "description": "Configuration for Compass real estate agent directory pages",
  "markers": {
    "start": {
      "type": "text",
      "value": "Agents Found"
    },
    "end": {
      "type": "coordinate",
      "value": {"x": 1810, "y": 5460}
    }
  },
  "scrollBehavior": {
    "enabled": true,
    "scrollDelay": 500,
    "maxScrolls": 50
  },
  "parsing": {
    "emailDomain": "compass.com",
    "nameBeforeEmail": true,
    "profileUrlPatterns": ["/agents/"]
  },
  "pagination": {
    "enabled": true,
    "type": "auto",
    "comment": "Compass uses URL parameter-based pagination",
    "patterns": {
      "pageParameterName": null,
      "comment": "Auto-detect pagination pattern"
    },
    "emptyPageIndicators": [
      "No agents found",
      "0 Agents Found"
    ],
    "validation": {
      "checkDuplicateContent": true,
      "minEmailsPerPage": 1
    }
  }
}
