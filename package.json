{
  "name": "universal-scraper",
  "version": "1.0.0",
  "description": "Universal professional directory scraper with intelligent extraction and PDF fallback",
  "main": "orchestrator.js",
  "scripts": {
    "start": "node orchestrator.js",
    "test": "node tests/scraper-test.js",
    "test:pdf": "node tests/pdf-scraper-test.js",
    "test:merger": "node tests/data-merger-test.js",
    "test:all": "node tests/scraper-test.js && node tests/pdf-scraper-test.js && node tests/data-merger-test.js",
    "test:nav": "node tests/run-navigation-tests.js",
    "test:nav:scroll": "node tests/run-navigation-tests.js --type scroll",
    "test:nav:page": "node tests/run-navigation-tests.js --type pagination",
    "test:nav:quick": "node tests/run-navigation-tests.js --quick",
    "test:nav:verbose": "node tests/run-navigation-tests.js --verbose"
  },
  "keywords": [
    "scraper",
    "puppeteer",
    "automation",
    "directory",
    "pdf"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "googleapis": "^128.0.0",
    "pdf-parse": "^1.1.4",
    "puppeteer": "^21.11.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "selenium-webdriver": "^4.39.0",
    "tesseract.js": "^6.0.1",
    "winston": "^3.18.3"
  }
}
